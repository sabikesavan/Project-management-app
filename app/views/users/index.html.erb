<h1>Home</h1>
<% if user_signed_in? %>
  <%= button_to 'Project', projects_path(id: current_user.id), method: :get %><br>
  <%= button_to 'Logout', destroy_user_session_path, method: :delete %>
<% else %>
  <%= button_to 'Signup', new_user_registration_path, method: :get %><br>
  <%= button_to 'Login', new_user_session_path, method: :get %>
<% end %>
